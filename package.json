{
    "name": "photo-library",
    "productName": "Photo Library",
    "version": "1.0.0",
    "description": "Sort photos automatically for the user!",
    "main": "index.js",
    "scripts": {
        "start": "electron .",
        "debug": "electron . --debug",
        "pack": "electron-builder --dir",
