    },
    "repository": {
        "type": "git",
        "url": "https://github.com/andrewgremlich/photo-library.git"
    },
    "dependencies": {
        "exif": "^0.6.0",
        "mkdirp": "^0.5.1"
    },
    "devDependencies": {
        "electron": "^6.0.3",
        "electron-builder": "^21.2.0"
    }
}
